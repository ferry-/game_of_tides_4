{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "align": true,
    "indent": [true, "spaces", 2],
    "max-classes-per-file": false,
    "max-line-length": [true, 80],
    "no-consecutive-blank-lines": false,
    "no-console": false,
    "no-reference": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "double"],
    "whitespace": [false, "check-branch"]
  }
}
